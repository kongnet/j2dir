/*复选框*/
.ui_switch::before {
    content: '';
    display: block;
    width: 13px;
    height: 13px;
    border-radius: 13px;
    background-color: #fff;
    -webkit-transition: margin-left .2s;
    transition: margin-left .2s;
    }
.ui_switch {
    display: inline-block;
    width: 35px;
    height: 17px;
    border: 2px solid;
    border-radius: 17px;
    background-color:#ccc;
    box-sizing: border-box;
    color: #b5b5b5;
    transition: all .2s;
    cursor: pointer;
}

:checked + .ui_switch::before {
  margin-left: 18px;
}
:checked + .ui_switch{
	background-color: #4DB4FF;
}
:disabled + .ui_switch{
	border:2px solid #CCCCCC;
}
/*大复选框*/
