/*单选样式*/
.ro_ch::before{
	content: '';
	display: block;
	width: 5px;
	height:5px;
	margin:0 auto;
	margin-top:5px;
	transition:all .5s;
	border-radius: 10px;
}
.ro_ch{
	width:15px;
	height:15px;
	border-radius:15px;
	border-width:1px;
	border:1px solid #737373;
	border-style:solid;
	cursor: pointer;
	float:left;
}
:checked + .ro_ch::before{
	background-color: #4db4ff;
}
:disabled + .ro_ch::before{
	background-color:#cccccc;
}
:disabled + .ro_ch{
	border:1px solid #cccccc;
}
